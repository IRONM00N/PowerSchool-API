{
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/node_modules": true,
		"**/dist/**/*.js.map": true,
		"**/dist/**/*.ts.map": true
	},
	"editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.format": true
  },
}