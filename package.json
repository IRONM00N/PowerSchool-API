{
	"name": "powerschool-api",
	"version": "1.1.0",
	"description": "A Node.js library for interacting with the PowerSchool API.",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"test": "node --enable-source-maps test.js",
		"format": "yarn prettier . --write",
		"build": "yarn rimraf dist && yarn tsc",
		"prepare": "yarn build"
	},
	"repository": {
		"type": "git",
		"url": "https://www.github.com/aydenp/PowerSchool-API.git"
	},
	"keywords": [
		"powerschool",
		"api",
		"wrapper",
		"http",
		"https",
		"soap",
		"xml"
	],
	"author": "Ayden <madebyayden+npm@icloud.com> (https://www.madebyayden.co)",
	"license": "MIT",
	"dependencies": {
		"@types/request": "^2.48.8",
		"soap": "0.39.0"
	},
	"peerDependencies": {
		"tslib": ">=2.3.1"
	},
	"devDependencies": {
		"@types/node": "^18.11.18",
		"@types/sax": "^1.2.4",
		"husky": "^8.0.3",
		"prettier": "^2.8.3",
		"readline-sync": "^1.4.10",
		"rimraf": "^4.1.2",
		"tslib": "^2.5.0",
		"typedoc": "^0.23.24",
		"typescript": "^4.9.4"
	},
	"prettier": {
		"useTabs": true,
		"trailingComma": "es5",
		"arrowParens": "always",
		"printWidth": 120
	},
	"packageManager": "yarn@3.3.1"
}
