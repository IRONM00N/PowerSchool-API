{
	"compilerOptions": {
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"target": "ESNext",
		"lib": ["ESNext"],
		"types": ["node"],
		"baseUrl": "./",
		"outDir": "dist",
		"forceConsistentCasingInFileNames": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"noErrorTruncation": true,
		"useUnknownInCatchVariables": false,
		"strict": true,
		"noImplicitOverride": true,
		"declarationMap": true,
		"declaration": true,
		"sourceMap": true,
		"noEmitHelpers": true,
		"importHelpers": true
	}
}
